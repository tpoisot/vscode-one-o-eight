{
    "name": "One O Eight Safe",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#313131"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#515151"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#FE6100"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#FE8133",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#DC267F"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#FFB000"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#313131",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#313131"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#83A5FF"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#404040"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#FFC033"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#785EF0"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#937EF3"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#648FFF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#648FFF"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#785EF0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#785EF0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#648FFF"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#648FFF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#785EF0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#FFB000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#FE8133",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#83A5FF",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#785EF0",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#313131",
                "fontStyle": "underline"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#515151",
        "tab.unfocusedActiveBackground": "#FDFDFD",
        "tab.inactiveBackground": "#D5D5D5",
        "notebookStatusErrorIcon.foreground": "#E35199",
        "tab.border": "#E8E8E8",
        "editor.foreground": "#212121",
        "notebook.focusedCellBorder": "#E8E8E8",
        "editorGroupHeader.tabsBackground": "#E8E8E8",
        "editorGutter.modifiedBackground": "#83A5FF",
        "icon.foreground": "#313131",
        "editor.background": "#FDFDFD",
        "diffEditor.diagonalFill": "#60606044",
        "statusBar.background": "#E8E8E8",
        "gitDecoration.untrackedResourceForeground": "#785EF0",
        "activityBar.foreground": "#313131",
        "gitDecoration.addedResourceForeground": "#FFB000",
        "editor.selectionBackground": "#51515166",
        "editorGutter.addedBackground": "#FFC033",
        "editor.lineHighlightBorder": "#FDFDFD",
        "notebookStatusSuccessIcon.foreground": "#FFC033",
        "editorGutter.deletedBackground": "#E35199",
        "titleBar.activeBackground": "#E8E8E8",
        "sideBar.border": "#E8E8E8",
        "notebook.cellHoverBackground": "#E8E8E855",
        "gitDecoration.modifiedResourceForeground": "#648FFF",
        "editorLineNumber.foreground": "#606060",
        "foreground": "#212121",
        "notebook.focusedEditorBorder": "#785EF0",
        "editorCursor.foreground": "#FE6100",
        "sideBar.background": "#E8E8E8",
        "notebookStatusRunningIcon.foreground": "#83A5FF",
        "list.highlightForeground": "#212121",
        "tab.activeBorderTop": "#937EF3",
        "notebook.focusedCellBackground": "#E8E8E8",
        "sideBar.dropBackground": "#937EF3",
        "statusBar.foreground": "#212121",
        "editorLineNumber.activeForeground": "#404040",
        "diffEditor.removedTextBackground": "#FE813322",
        "titleBar.inactiveBackground": "#E8E8E8",
        "diffEditor.insertedTextBackground": "#83A5FF22",
        "activityBar.background": "#E8E8E8",
        "editorGroup.border": "#E8E8E8",
        "gitDecoration.deletedResourceForeground": "#DC267F"
    },
    "type": "light"
}
