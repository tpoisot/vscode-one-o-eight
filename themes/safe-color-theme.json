{
    "name": "Safe",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#6A6A6A"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#FE6100"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#E24D96",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#DC267F"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#FFB000"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#464646",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#464646"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#80A3FF"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#6A6A6A"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#FFBE2E"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#785EF0"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#907BF2"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#648FFF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#648FFF"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#785EF0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#785EF0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#648FFF"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#648FFF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#785EF0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#FFB000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#FE7D2E",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#80A3FF",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#785EF0",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#464646",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#8F8F8F"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#C6C6C6",
        "tab.unfocusedActiveBackground": "#FDFDFD",
        "tab.inactiveBackground": "#DCDCDC",
        "notebookStatusErrorIcon.foreground": "#E24D96",
        "button.hoverBackground": "#648FFF",
        "dropdown.foreground": "#212121",
        "tab.border": "#E7E7E7",
        "badge.foreground": "#212121",
        "badge.background": "#80A3FF",
        "editor.foreground": "#212121",
        "notebook.focusedCellBorder": "#E7E7E7",
        "input.foreground": "#212121",
        "dropdown.background": "#C6C6C6",
        "button.foreground": "#212121",
        "editorGroupHeader.tabsBackground": "#E7E7E7",
        "editorGutter.modifiedBackground": "#80A3FF",
        "icon.foreground": "#212121",
        "editor.background": "#FDFDFD",
        "diffEditor.diagonalFill": "#D1D1D144",
        "statusBar.background": "#E7E7E7",
        "dropdown.border": "#C6C6C6",
        "dropdown.listBackground": "#C6C6C6",
        "gitDecoration.untrackedResourceForeground": "#DC267F",
        "activityBar.foreground": "#212121",
        "gitDecoration.addedResourceForeground": "#FFB000",
        "editor.selectionBackground": "#D1D1D166",
        "editorGutter.addedBackground": "#FFBE2E",
        "editor.lineHighlightBorder": "#FDFDFD",
        "notebookStatusSuccessIcon.foreground": "#FFBE2E",
        "editorGutter.deletedBackground": "#E24D96",
        "titleBar.activeBackground": "#E7E7E7",
        "input.background": "#FDFDFD",
        "sideBar.border": "#E7E7E7",
        "notebook.cellHoverBackground": "#E7E7E755",
        "gitDecoration.modifiedResourceForeground": "#648FFF",
        "window.activeBorder": "#E7E7E7",
        "editorLineNumber.foreground": "#DCDCDC",
        "foreground": "#212121",
        "notebook.focusedEditorBorder": "#785EF0",
        "editorCursor.foreground": "#907BF2",
        "list.activeSelectionBackground": "#D1D1D1",
        "button.border": "#80A3FF",
        "activityBar.inactiveForeground": "#BBBBBB",
        "titleBar.inactiveForeground": "#212121",
        "sideBar.background": "#E7E7E7",
        "notebookStatusRunningIcon.foreground": "#80A3FF",
        "list.highlightForeground": "#212121",
        "button.background": "#80A3FF",
        "tab.activeBorderTop": "#80A3FF",
        "notebook.focusedCellBackground": "#E7E7E7",
        "tab.activeForeground": "#212121",
        "titleBar.activeForeground": "#212121",
        "sideBar.dropBackground": "#80A3FF",
        "statusBar.foreground": "#212121",
        "editorLineNumber.activeForeground": "#BBBBBB",
        "diffEditor.removedTextBackground": "#FE7D2E22",
        "titleBar.inactiveBackground": "#E7E7E7",
        "diffEditor.insertedTextBackground": "#80A3FF22",
        "activityBar.background": "#E7E7E7",
        "editorGroup.border": "#E7E7E7",
        "gitDecoration.deletedResourceForeground": "#DC267F",
        "tab.inactiveForeground": "#212121",
        "list.activeSelectionForeground": "#212121"
    },
    "type": "light"
}
