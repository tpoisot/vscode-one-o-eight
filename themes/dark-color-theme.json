{
    "name": "Dark",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#B3B2B4"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#DFB674"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#C7878B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#EA9C9C"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#7BD078"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#D6D4D3",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#D6D4D3"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#7EAFBF"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#B3B2B4"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#6CB26D"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#D7A7E1"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#B890C3"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#91CCDC",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#91CCDC"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#D7A7E1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#D7A7E1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#91CCDC"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#91CCDC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#D7A7E1",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#7BD078",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#BE9D6A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#7EAFBF",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#D7A7E1",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#D6D4D3",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#919096"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#535360",
        "tab.unfocusedActiveBackground": "#2A2A3B",
        "tab.inactiveBackground": "#3F3E4D",
        "notebookStatusErrorIcon.foreground": "#C7878B",
        "tab.border": "#343444",
        "badge.foreground": "#F8F6F1",
        "badge.background": "#7EAFBF",
        "editor.foreground": "#F8F6F1",
        "notebook.focusedCellBorder": "#343444",
        "editorGroupHeader.tabsBackground": "#343444",
        "editorGutter.modifiedBackground": "#7EAFBF",
        "icon.foreground": "#F8F6F1",
        "editor.background": "#2A2A3B",
        "diffEditor.diagonalFill": "#49485644",
        "statusBar.background": "#343444",
        "gitDecoration.untrackedResourceForeground": "#EA9C9C",
        "activityBar.foreground": "#F8F6F1",
        "gitDecoration.addedResourceForeground": "#7BD078",
        "editor.selectionBackground": "#49485666",
        "editorGutter.addedBackground": "#6CB26D",
        "editor.lineHighlightBorder": "#2A2A3B",
        "notebookStatusSuccessIcon.foreground": "#6CB26D",
        "editorGutter.deletedBackground": "#C7878B",
        "titleBar.activeBackground": "#343444",
        "sideBar.border": "#343444",
        "notebook.cellHoverBackground": "#34344455",
        "gitDecoration.modifiedResourceForeground": "#91CCDC",
        "editorLineNumber.foreground": "#3F3E4D",
        "foreground": "#F8F6F1",
        "notebook.focusedEditorBorder": "#D7A7E1",
        "editorCursor.foreground": "#B890C3",
        "activityBar.inactiveForeground": "#5D5D69",
        "sideBar.background": "#343444",
        "notebookStatusRunningIcon.foreground": "#7EAFBF",
        "list.highlightForeground": "#F8F6F1",
        "tab.activeBorderTop": "#7EAFBF",
        "notebook.focusedCellBackground": "#343444",
        "sideBar.dropBackground": "#7EAFBF",
        "statusBar.foreground": "#F8F6F1",
        "editorLineNumber.activeForeground": "#5D5D69",
        "diffEditor.removedTextBackground": "#BE9D6A22",
        "titleBar.inactiveBackground": "#343444",
        "diffEditor.insertedTextBackground": "#7EAFBF22",
        "activityBar.background": "#343444",
        "editorGroup.border": "#343444",
        "gitDecoration.deletedResourceForeground": "#EA9C9C"
    },
    "type": "dark"
}
