{
    "name": "Dark",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#B5B7BA"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#EBCC7F"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#D5A9AC",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#F7C1C1"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#95E086"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#D4D7D6",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#D4D7D6"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#8AB9D4"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#B5B7BA"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#85C27B"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#FFB7EF"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#DCA0D1"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#9CD5F2",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#9CD5F2"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#FFB7EF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#FFB7EF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#9CD5F2"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#9CD5F2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#FFB7EF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#95E086",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#CBB275",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#8AB9D4",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#FFB7EF",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#D4D7D6",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#96989E"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#5A5A67",
        "tab.unfocusedActiveBackground": "#3A3A4A",
        "tab.inactiveBackground": "#4A4A58",
        "notebookStatusErrorIcon.foreground": "#D5A9AC",
        "tab.border": "#424251",
        "badge.foreground": "#F3F6F2",
        "badge.background": "#8AB9D4",
        "editor.foreground": "#F3F6F2",
        "notebook.focusedCellBorder": "#424251",
        "editorGroupHeader.tabsBackground": "#424251",
        "editorGutter.modifiedBackground": "#8AB9D4",
        "icon.foreground": "#F3F6F2",
        "editor.background": "#3A3A4A",
        "diffEditor.diagonalFill": "#52525F44",
        "statusBar.background": "#424251",
        "gitDecoration.untrackedResourceForeground": "#F7C1C1",
        "activityBar.foreground": "#F3F6F2",
        "gitDecoration.addedResourceForeground": "#95E086",
        "editor.selectionBackground": "#52525F66",
        "editorGutter.addedBackground": "#85C27B",
        "editor.lineHighlightBorder": "#3A3A4A",
        "notebookStatusSuccessIcon.foreground": "#85C27B",
        "editorGutter.deletedBackground": "#D5A9AC",
        "titleBar.activeBackground": "#424251",
        "sideBar.border": "#424251",
        "notebook.cellHoverBackground": "#42425155",
        "gitDecoration.modifiedResourceForeground": "#9CD5F2",
        "editorLineNumber.foreground": "#4A4A58",
        "foreground": "#F3F6F2",
        "notebook.focusedEditorBorder": "#FFB7EF",
        "editorCursor.foreground": "#DCA0D1",
        "activityBar.inactiveForeground": "#62626E",
        "sideBar.background": "#424251",
        "notebookStatusRunningIcon.foreground": "#8AB9D4",
        "list.highlightForeground": "#F3F6F2",
        "tab.activeBorderTop": "#8AB9D4",
        "notebook.focusedCellBackground": "#424251",
        "sideBar.dropBackground": "#8AB9D4",
        "statusBar.foreground": "#F3F6F2",
        "editorLineNumber.activeForeground": "#62626E",
        "diffEditor.removedTextBackground": "#CBB27522",
        "titleBar.inactiveBackground": "#424251",
        "diffEditor.insertedTextBackground": "#8AB9D422",
        "activityBar.background": "#424251",
        "editorGroup.border": "#424251",
        "gitDecoration.deletedResourceForeground": "#F7C1C1"
    },
    "type": "dark"
}
