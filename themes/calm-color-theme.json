{
    "name": "Calm",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#56665A"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#514548"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#9F5177",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#8C2D5B"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#165D34"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#2E4234",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#2E4234"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#3F746E"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#56665A"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#3F7957"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#16536B"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#3F7084"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#165850",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#165850"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#16536B",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#16536B",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#165850"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#165850",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#16536B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#165D34",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#6F6567",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#3F746E",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#16536B",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#2E4234",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#7E8A81"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#C4C8C3",
        "tab.unfocusedActiveBackground": "#F7F6F5",
        "tab.inactiveBackground": "#D8DAD7",
        "notebookStatusErrorIcon.foreground": "#9F5177",
        "tab.border": "#E2E3E1",
        "badge.foreground": "#061E0D",
        "badge.background": "#3F746E",
        "editor.foreground": "#061E0D",
        "notebook.focusedCellBorder": "#E2E3E1",
        "editorGroupHeader.tabsBackground": "#E2E3E1",
        "editorGutter.modifiedBackground": "#3F746E",
        "icon.foreground": "#061E0D",
        "editor.background": "#F7F6F5",
        "diffEditor.diagonalFill": "#CED1CD44",
        "statusBar.background": "#E2E3E1",
        "gitDecoration.untrackedResourceForeground": "#8C2D5B",
        "activityBar.foreground": "#061E0D",
        "gitDecoration.addedResourceForeground": "#165D34",
        "editor.selectionBackground": "#CED1CD66",
        "editorGutter.addedBackground": "#3F7957",
        "editor.lineHighlightBorder": "#F7F6F5",
        "notebookStatusSuccessIcon.foreground": "#3F7957",
        "editorGutter.deletedBackground": "#9F5177",
        "titleBar.activeBackground": "#E2E3E1",
        "sideBar.border": "#E2E3E1",
        "notebook.cellHoverBackground": "#E2E3E155",
        "gitDecoration.modifiedResourceForeground": "#165850",
        "editorLineNumber.foreground": "#D8DAD7",
        "foreground": "#061E0D",
        "notebook.focusedEditorBorder": "#16536B",
        "editorCursor.foreground": "#3F7084",
        "activityBar.inactiveForeground": "#B9BEB9",
        "sideBar.background": "#E2E3E1",
        "notebookStatusRunningIcon.foreground": "#3F746E",
        "list.highlightForeground": "#061E0D",
        "tab.activeBorderTop": "#3F746E",
        "notebook.focusedCellBackground": "#E2E3E1",
        "sideBar.dropBackground": "#3F746E",
        "statusBar.foreground": "#061E0D",
        "editorLineNumber.activeForeground": "#B9BEB9",
        "diffEditor.removedTextBackground": "#6F656722",
        "titleBar.inactiveBackground": "#E2E3E1",
        "diffEditor.insertedTextBackground": "#3F746E22",
        "activityBar.background": "#E2E3E1",
        "editorGroup.border": "#E2E3E1",
        "gitDecoration.deletedResourceForeground": "#8C2D5B"
    },
    "type": "light"
}
