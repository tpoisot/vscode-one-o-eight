{
    "name": "Calm",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#70686C"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#524648"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#A05278",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#8C2D5B"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#186035"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#4D4349",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#4D4349"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#3F7A73"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#70686C"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#417C58"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#145B77"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#3D788F"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#165E56",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#165E56"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#145B77",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#145B77",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#165E56"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#165E56",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#145B77",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#186035",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#706668",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#3F7A73",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#145B77",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#4D4349",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#4D4349"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#847C80",
        "tab.unfocusedActiveBackground": "#FAF9F9",
        "tab.inactiveBackground": "#A19C9F",
        "notebookStatusErrorIcon.foreground": "#A05278",
        "tab.border": "#BEBBBD",
        "badge.foreground": "#2B1F26",
        "badge.background": "#3D788F",
        "editor.foreground": "#2B1F26",
        "notebook.focusedCellBorder": "#BEBBBD",
        "editorGroupHeader.tabsBackground": "#BEBBBD",
        "editorGutter.modifiedBackground": "#3F7A73",
        "icon.foreground": "#3D788F",
        "editor.background": "#FAF9F9",
        "diffEditor.diagonalFill": "#70686C44",
        "statusBar.background": "#BEBBBD",
        "gitDecoration.untrackedResourceForeground": "#145B77",
        "activityBar.foreground": "#3D788F",
        "gitDecoration.addedResourceForeground": "#186035",
        "editor.selectionBackground": "#928C9066",
        "editorGutter.addedBackground": "#417C58",
        "editor.lineHighlightBorder": "#FAF9F9",
        "notebookStatusSuccessIcon.foreground": "#417C58",
        "editorGutter.deletedBackground": "#A05278",
        "titleBar.activeBackground": "#BEBBBD",
        "sideBar.border": "#BEBBBD",
        "notebook.cellHoverBackground": "#BEBBBD55",
        "gitDecoration.modifiedResourceForeground": "#165E56",
        "editorLineNumber.foreground": "#928C90",
        "foreground": "#2B1F26",
        "notebook.focusedEditorBorder": "#145B77",
        "editorCursor.foreground": "#4D4349",
        "activityBar.inactiveForeground": "#675D62",
        "sideBar.background": "#BEBBBD",
        "notebookStatusRunningIcon.foreground": "#3F7A73",
        "list.highlightForeground": "#2B1F26",
        "tab.activeBorderTop": "#3D788F",
        "notebook.focusedCellBackground": "#BEBBBD",
        "sideBar.dropBackground": "#3D788F",
        "statusBar.foreground": "#2B1F26",
        "editorLineNumber.activeForeground": "#4D4349",
        "diffEditor.removedTextBackground": "#70666822",
        "titleBar.inactiveBackground": "#BEBBBD",
        "diffEditor.insertedTextBackground": "#3F7A7322",
        "activityBar.background": "#BEBBBD",
        "editorGroup.border": "#BEBBBD",
        "gitDecoration.deletedResourceForeground": "#8C2D5B"
    },
    "type": "light"
}
