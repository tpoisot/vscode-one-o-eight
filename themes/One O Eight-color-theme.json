{
	"name": "One O Eight",
	"type": "light",
	"base": {
		"purple": "#7b1fa2",
		"blue": "#1976d2",
		"green": "#388e3c",
		"yellow": "#f57c00",
		"red": "#e64a19"
	},
	"comments": {
		"darkest": "#282828",
		"darker": "#5A5A5A",
		"dark": "#8C8C8C",
		"midpoint": "#bebebe",
		"light": "#D3D3D3",
		"lighter": "#E9E9E9",
		"lightest": "#FEFEFE"
	},
	"colors": {
		"editor.background": "#fefefe",
		"editor.foreground": "#282828",
		"activityBar.background": "#d3d3d3",
		"activityBar.foreground": "#5a5a5a",
		"activityBar.border": "#d3d3d3",
		"sideBar.background": "#e9e9e9",
		"sideBar.border": "#d3d3d3",
		"sideBar.foreground": "#5a5a5a",
		"selection.background": "#f57c0033",
		"button.background": "#5a5a5a",
		"button.foreground": "#fefefe",
		"statusBar.background": "#d3d3d3",
		"statusBar.foreground": "#282828",
		"titleBar.activeBackground": "#d3d3d3",
		"titleBar.activeForeground": "#282828"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": ["comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#bebebe"
			}
		},
		{
			"name": "Comments punctuation",
			"scope": [
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#5a5a5a"
			}
		},
		{
			"name": "Control keywords",
			"scope": [
				"keyword",
				"keyword.control.end"
			],
			"settings": {
				"foreground": "#1976d2"
			}
		},
		{
			"name": "Numbers and constants",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#388e3c"
			}
		},
		{
			"name": "Interpolated string",
			"scope": [
				"variable.interpolation"
			],
			"settings": {
				"foreground": "#f57c00",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Macro",
			"scope": [
				"support.function.macro"
			],
			"settings": {
				"foreground": "#7b1fa2",
				"fontStyle": ""
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"meta.bracket"
			],
			"settings": {
				"foreground": "#8c8c8c"
			}
		},
		{
			"name": "Boolean operators",
			"scope": [
				"keyword.operator.boolean"
			],
			"settings": {
				"foreground": "#f57c00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Type",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#f57c00"
			}
		},
		{
			"name": "Entity",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#e64a19"
			}
		},
		{
			"name": "Strint content",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#388e3c"
			}
		},
		{
			"name": "String begin and end",
			"scope": [
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#bebebe",
			}
		},
		{
			"name": "Markdown header definition",
			"scope": [
				"punctuation.definition.heading"
			],
			"settings": {
				"foreground": "#bebebe",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown header",
			"scope": [
				"entity.name.section"
			],
			"settings": {
				"foreground": "#1976d2",
				"fontStyle": "bold"
			}
		}
	]
}