{
    "name": "Seaside",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#706E7A"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#3FA382"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#5B947D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#387E62"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#D79C65"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#4C4B59",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#4C4B59"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#5694DC"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#706E7A"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#DDAD80"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#1F9DB4"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#46AEC1"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#327ED6",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#327ED6"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#1F9DB4",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#1F9DB4",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#327ED6"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#327ED6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#1F9DB4",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#D79C65",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#61B398",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#5694DC",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#1F9DB4",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#4C4B59",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#93919A"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#CECDD2",
        "tab.unfocusedActiveBackground": "#FBFAFA",
        "tab.inactiveBackground": "#E0DFE1",
        "notebookStatusErrorIcon.foreground": "#5B947D",
        "tab.border": "#E9E8E9",
        "badge.foreground": "#2A293A",
        "badge.background": "#5694DC",
        "editor.foreground": "#2A293A",
        "notebook.focusedCellBorder": "#E9E8E9",
        "editorGroupHeader.tabsBackground": "#E9E8E9",
        "editorGutter.modifiedBackground": "#5694DC",
        "icon.foreground": "#2A293A",
        "editor.background": "#FBFAFA",
        "diffEditor.diagonalFill": "#D7D6DA44",
        "statusBar.background": "#E9E8E9",
        "gitDecoration.untrackedResourceForeground": "#387E62",
        "activityBar.foreground": "#2A293A",
        "gitDecoration.addedResourceForeground": "#D79C65",
        "editor.selectionBackground": "#D7D6DA66",
        "editorGutter.addedBackground": "#DDAD80",
        "editor.lineHighlightBorder": "#FBFAFA",
        "notebookStatusSuccessIcon.foreground": "#DDAD80",
        "editorGutter.deletedBackground": "#5B947D",
        "titleBar.activeBackground": "#E9E8E9",
        "sideBar.border": "#E9E8E9",
        "notebook.cellHoverBackground": "#E9E8E955",
        "gitDecoration.modifiedResourceForeground": "#327ED6",
        "editorLineNumber.foreground": "#E0DFE1",
        "foreground": "#2A293A",
        "notebook.focusedEditorBorder": "#1F9DB4",
        "editorCursor.foreground": "#C4C4C9",
        "activityBar.inactiveForeground": "#C4C4C9",
        "sideBar.background": "#E9E8E9",
        "notebookStatusRunningIcon.foreground": "#5694DC",
        "list.highlightForeground": "#2A293A",
        "tab.activeBorderTop": "#5694DC",
        "notebook.focusedCellBackground": "#E9E8E9",
        "sideBar.dropBackground": "#5694DC",
        "statusBar.foreground": "#2A293A",
        "editorLineNumber.activeForeground": "#C4C4C9",
        "diffEditor.removedTextBackground": "#61B39822",
        "titleBar.inactiveBackground": "#E9E8E9",
        "diffEditor.insertedTextBackground": "#5694DC22",
        "activityBar.background": "#E9E8E9",
        "editorGroup.border": "#E9E8E9",
        "gitDecoration.deletedResourceForeground": "#387E62"
    },
    "type": "light"
}
