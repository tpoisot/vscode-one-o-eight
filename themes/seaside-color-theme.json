{
    "name": "Seaside",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#6B6D75"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#3FA382"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#5A947D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#387E62"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#D79C65"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#484A55",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#484A55"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#5594DC"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#6B6D75"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#DDAC7F"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#1F9DB4"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#46ADC0"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#327ED6",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#327ED6"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#1F9DB4",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#1F9DB4",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#327ED6"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#327ED6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#1F9DB4",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#D79C65",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#60B297",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#5594DC",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#1F9DB4",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#484A55",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#8E9096"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#CACBCD",
        "tab.unfocusedActiveBackground": "#F7F7F6",
        "tab.inactiveBackground": "#DCDCDD",
        "notebookStatusErrorIcon.foreground": "#5A947D",
        "tab.border": "#E5E5E5",
        "badge.foreground": "#252835",
        "badge.background": "#5594DC",
        "editor.foreground": "#252835",
        "notebook.focusedCellBorder": "#E5E5E5",
        "editorGroupHeader.tabsBackground": "#E5E5E5",
        "editorGutter.modifiedBackground": "#5594DC",
        "icon.foreground": "#252835",
        "editor.background": "#F7F7F6",
        "diffEditor.diagonalFill": "#D3D4D544",
        "statusBar.background": "#E5E5E5",
        "gitDecoration.untrackedResourceForeground": "#387E62",
        "activityBar.foreground": "#252835",
        "gitDecoration.addedResourceForeground": "#D79C65",
        "editor.selectionBackground": "#D3D4D566",
        "editorGutter.addedBackground": "#DDAC7F",
        "editor.lineHighlightBorder": "#F7F7F6",
        "notebookStatusSuccessIcon.foreground": "#DDAC7F",
        "editorGutter.deletedBackground": "#5A947D",
        "titleBar.activeBackground": "#E5E5E5",
        "sideBar.border": "#E5E5E5",
        "notebook.cellHoverBackground": "#E5E5E555",
        "gitDecoration.modifiedResourceForeground": "#327ED6",
        "editorLineNumber.foreground": "#DCDCDD",
        "foreground": "#252835",
        "notebook.focusedEditorBorder": "#1F9DB4",
        "editorCursor.foreground": "#46ADC0",
        "activityBar.inactiveForeground": "#C1C2C4",
        "sideBar.background": "#E5E5E5",
        "notebookStatusRunningIcon.foreground": "#5594DC",
        "list.highlightForeground": "#252835",
        "tab.activeBorderTop": "#5594DC",
        "notebook.focusedCellBackground": "#E5E5E5",
        "sideBar.dropBackground": "#5594DC",
        "statusBar.foreground": "#252835",
        "editorLineNumber.activeForeground": "#C1C2C4",
        "diffEditor.removedTextBackground": "#60B29722",
        "titleBar.inactiveBackground": "#E5E5E5",
        "diffEditor.insertedTextBackground": "#5594DC22",
        "activityBar.background": "#E5E5E5",
        "editorGroup.border": "#E5E5E5",
        "gitDecoration.deletedResourceForeground": "#387E62"
    },
    "type": "light"
}
