{
    "name": "Bright",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#626162"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#95431C"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#BF3C3C",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#B11111"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#0E6336"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#3A393A",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#3A393A"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#3B79A2"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#626162"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#397F5A"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#6F449A"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#8966AC"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#105B8D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#105B8D"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#6F449A",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#6F449A",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#105B8D"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#105B8D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#6F449A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#0E6336",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#A86545",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#3B79A2",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#6F449A",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#3A393A",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#898989"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#CCCCCC",
        "tab.unfocusedActiveBackground": "#FFFFFF",
        "tab.inactiveBackground": "#E1E0E1",
        "notebookStatusErrorIcon.foreground": "#BF3C3C",
        "tab.border": "#EBEAEB",
        "badge.foreground": "#131213",
        "badge.background": "#3B79A2",
        "editor.foreground": "#131213",
        "notebook.focusedCellBorder": "#EBEAEB",
        "editorGroupHeader.tabsBackground": "#EBEAEB",
        "editorGutter.modifiedBackground": "#3B79A2",
        "icon.foreground": "#131213",
        "editor.background": "#FFFFFF",
        "diffEditor.diagonalFill": "#D6D6D644",
        "statusBar.background": "#EBEAEB",
        "gitDecoration.untrackedResourceForeground": "#B11111",
        "activityBar.foreground": "#131213",
        "gitDecoration.addedResourceForeground": "#0E6336",
        "editor.selectionBackground": "#D6D6D666",
        "editorGutter.addedBackground": "#397F5A",
        "editor.lineHighlightBorder": "#FFFFFF",
        "notebookStatusSuccessIcon.foreground": "#397F5A",
        "editorGutter.deletedBackground": "#BF3C3C",
        "titleBar.activeBackground": "#EBEAEB",
        "sideBar.border": "#EBEAEB",
        "notebook.cellHoverBackground": "#EBEAEB55",
        "gitDecoration.modifiedResourceForeground": "#105B8D",
        "editorLineNumber.foreground": "#E1E0E1",
        "foreground": "#131213",
        "notebook.focusedEditorBorder": "#6F449A",
        "editorCursor.foreground": "#C2C1C2",
        "activityBar.inactiveForeground": "#C2C1C2",
        "sideBar.background": "#EBEAEB",
        "notebookStatusRunningIcon.foreground": "#3B79A2",
        "list.highlightForeground": "#131213",
        "tab.activeBorderTop": "#3B79A2",
        "notebook.focusedCellBackground": "#EBEAEB",
        "sideBar.dropBackground": "#3B79A2",
        "statusBar.foreground": "#131213",
        "editorLineNumber.activeForeground": "#C2C1C2",
        "diffEditor.removedTextBackground": "#A8654522",
        "titleBar.inactiveBackground": "#EBEAEB",
        "diffEditor.insertedTextBackground": "#3B79A222",
        "activityBar.background": "#EBEAEB",
        "editorGroup.border": "#EBEAEB",
        "gitDecoration.deletedResourceForeground": "#B11111"
    },
    "type": "light"
}
