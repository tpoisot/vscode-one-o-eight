{
    "name": "Bright",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#6C6D6E"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#814313"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#BE3D3D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#B01212"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#0E6336"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#47484A",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#47484A"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#3B79A2"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#6C6D6E"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#397F5A"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#6F449A"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#8966AC"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#105B8D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#105B8D"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#6F449A",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#6F449A",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#105B8D"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#105B8D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#6F449A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#0E6336",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#98653D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#3B79A2",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#6F449A",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#47484A",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#909292"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#D0D0D1",
        "tab.unfocusedActiveBackground": "#FFFFFF",
        "tab.inactiveBackground": "#E3E3E3",
        "notebookStatusErrorIcon.foreground": "#BE3D3D",
        "tab.border": "#ECECEC",
        "badge.foreground": "#222426",
        "badge.background": "#3B79A2",
        "editor.foreground": "#222426",
        "notebook.focusedCellBorder": "#ECECEC",
        "editorGroupHeader.tabsBackground": "#ECECEC",
        "editorGutter.modifiedBackground": "#3B79A2",
        "icon.foreground": "#222426",
        "editor.background": "#FFFFFF",
        "diffEditor.diagonalFill": "#D9D9DA44",
        "statusBar.background": "#ECECEC",
        "gitDecoration.untrackedResourceForeground": "#B01212",
        "activityBar.foreground": "#222426",
        "gitDecoration.addedResourceForeground": "#0E6336",
        "editor.selectionBackground": "#D9D9DA66",
        "editorGutter.addedBackground": "#397F5A",
        "editor.lineHighlightBorder": "#FFFFFF",
        "notebookStatusSuccessIcon.foreground": "#397F5A",
        "editorGutter.deletedBackground": "#BE3D3D",
        "titleBar.activeBackground": "#ECECEC",
        "sideBar.border": "#ECECEC",
        "notebook.cellHoverBackground": "#ECECEC55",
        "gitDecoration.modifiedResourceForeground": "#105B8D",
        "editorLineNumber.foreground": "#E3E3E3",
        "foreground": "#222426",
        "notebook.focusedEditorBorder": "#6F449A",
        "editorCursor.foreground": "#8966AC",
        "activityBar.inactiveForeground": "#C6C6C7",
        "sideBar.background": "#ECECEC",
        "notebookStatusRunningIcon.foreground": "#3B79A2",
        "list.highlightForeground": "#222426",
        "tab.activeBorderTop": "#3B79A2",
        "notebook.focusedCellBackground": "#ECECEC",
        "sideBar.dropBackground": "#3B79A2",
        "statusBar.foreground": "#222426",
        "editorLineNumber.activeForeground": "#C6C6C7",
        "diffEditor.removedTextBackground": "#98653D22",
        "titleBar.inactiveBackground": "#ECECEC",
        "diffEditor.insertedTextBackground": "#3B79A222",
        "activityBar.background": "#ECECEC",
        "editorGroup.border": "#ECECEC",
        "gitDecoration.deletedResourceForeground": "#B01212"
    },
    "type": "light"
}
