{
    "name": "Light",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#6C6C6C"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#945604"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#B93953",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#AB0F2E"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#156038"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#474747",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#474747"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#457798"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#6C6C6C"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#3E7C5B"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#69468D"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#8366A1"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#1D5A83",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#1D5A83"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#69468D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#69468D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#1D5A83"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#1D5A83",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#69468D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#156038",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#A67330",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#457798",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#69468D",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#474747",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#909090"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#D5D5D5",
        "tab.unfocusedActiveBackground": "#FBFBFB",
        "tab.inactiveBackground": "#E0E0E0",
        "notebookStatusErrorIcon.foreground": "#B93953",
        "tab.border": "#E8E8E8",
        "badge.foreground": "#242424",
        "badge.background": "#457798",
        "editor.foreground": "#242424",
        "notebook.focusedCellBorder": "#E8E8E8",
        "editorGroupHeader.tabsBackground": "#E8E8E8",
        "editorGutter.modifiedBackground": "#457798",
        "icon.foreground": "#242424",
        "editor.background": "#FBFBFB",
        "diffEditor.diagonalFill": "#D5D5D544",
        "statusBar.background": "#E8E8E8",
        "gitDecoration.untrackedResourceForeground": "#69468D",
        "activityBar.foreground": "#242424",
        "gitDecoration.addedResourceForeground": "#156038",
        "editor.selectionBackground": "#E0E0E066",
        "editorGutter.addedBackground": "#3E7C5B",
        "editor.lineHighlightBorder": "#FBFBFB",
        "notebookStatusSuccessIcon.foreground": "#3E7C5B",
        "editorGutter.deletedBackground": "#B93953",
        "titleBar.activeBackground": "#E8E8E8",
        "sideBar.border": "#E8E8E8",
        "notebook.cellHoverBackground": "#E8E8E855",
        "gitDecoration.modifiedResourceForeground": "#1D5A83",
        "editorLineNumber.foreground": "#E0E0E0",
        "foreground": "#242424",
        "notebook.focusedEditorBorder": "#69468D",
        "editorCursor.foreground": "#C3C3C3",
        "activityBar.inactiveForeground": "#C3C3C3",
        "sideBar.background": "#E8E8E8",
        "notebookStatusRunningIcon.foreground": "#457798",
        "list.highlightForeground": "#242424",
        "tab.activeBorderTop": "#457798",
        "notebook.focusedCellBackground": "#E8E8E8",
        "sideBar.dropBackground": "#457798",
        "statusBar.foreground": "#242424",
        "editorLineNumber.activeForeground": "#C3C3C3",
        "diffEditor.removedTextBackground": "#A6733022",
        "titleBar.inactiveBackground": "#E8E8E8",
        "diffEditor.insertedTextBackground": "#45779822",
        "activityBar.background": "#E8E8E8",
        "editorGroup.border": "#E8E8E8",
        "gitDecoration.deletedResourceForeground": "#AB0F2E"
    },
    "type": "light"
}
