{
    "name": "One O Eight Light",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#626262"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#959595"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#D35400"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#DB7532",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#DC143C"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#1E824C"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#626262",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#626262"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#508FB9"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#7B7B7B"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#4A9A6F"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#8859B6"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#9F79C4"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#2574A9",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#2574A9"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#8859B6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#8859B6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#2574A9"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#2574A9",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#8859B6",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#1E824C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#DB7532",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#508FB9",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#8859B6",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#626262",
                "fontStyle": "underline"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#959595",
        "tab.unfocusedActiveBackground": "#FBFBFB",
        "tab.inactiveBackground": "#D3D3D3",
        "notebookStatusErrorIcon.foreground": "#E24262",
        "tab.border": "#E7E7E7",
        "editor.foreground": "#2F2F2F",
        "notebook.focusedCellBorder": "#E7E7E7",
        "editorGroupHeader.tabsBackground": "#E7E7E7",
        "editorGutter.modifiedBackground": "#508FB9",
        "icon.foreground": "#626262",
        "editor.background": "#FBFBFB",
        "diffEditor.diagonalFill": "#AFAFAF44",
        "statusBar.background": "#E7E7E7",
        "gitDecoration.untrackedResourceForeground": "#8859B6",
        "activityBar.foreground": "#626262",
        "gitDecoration.addedResourceForeground": "#1E824C",
        "editor.selectionBackground": "#95959566",
        "editorGutter.addedBackground": "#4A9A6F",
        "editor.lineHighlightBorder": "#FBFBFB",
        "notebookStatusSuccessIcon.foreground": "#4A9A6F",
        "editorGutter.deletedBackground": "#E24262",
        "titleBar.activeBackground": "#E7E7E7",
        "sideBar.border": "#E7E7E7",
        "notebook.cellHoverBackground": "#E7E7E755",
        "gitDecoration.modifiedResourceForeground": "#2574A9",
        "editorLineNumber.foreground": "#AFAFAF",
        "foreground": "#2F2F2F",
        "notebook.focusedEditorBorder": "#8859B6",
        "editorCursor.foreground": "#D35400",
        "sideBar.background": "#E7E7E7",
        "notebookStatusRunningIcon.foreground": "#508FB9",
        "list.highlightForeground": "#2F2F2F",
        "tab.activeBorderTop": "#9F79C4",
        "notebook.focusedCellBackground": "#E7E7E7",
        "sideBar.dropBackground": "#9F79C4",
        "statusBar.foreground": "#2F2F2F",
        "editorLineNumber.activeForeground": "#7B7B7B",
        "diffEditor.removedTextBackground": "#DB753222",
        "titleBar.inactiveBackground": "#E7E7E7",
        "diffEditor.insertedTextBackground": "#508FB922",
        "activityBar.background": "#E7E7E7",
        "editorGroup.border": "#E7E7E7",
        "gitDecoration.deletedResourceForeground": "#DC143C"
    },
    "type": "light"
}
