{
    "name": "Angular Light",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#606073"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#694B19"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#98547C",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#853261"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#25591B"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#3B3C52",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#3B3C52"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#427375"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#606073"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#4A7442"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#543DA1"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#705DB0"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#1B5759",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#1B5759"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#543DA1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#543DA1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#1B5759"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#1B5759",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#543DA1",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#25591B",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#816941",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#427375",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#543DA1",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#3B3C52",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#848494"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#BBBAC4",
        "tab.unfocusedActiveBackground": "#F1F0F5",
        "notebookStatusErrorIcon.foreground": "#98547C",
        "tab.inactiveBackground": "#D0CFD8",
        "button.hoverBackground": "#1B5759",
        "dropdown.foreground": "#171832",
        "tab.border": "#DBDAE2",
        "badge.foreground": "#171832",
        "badge.background": "#4A7442",
        "editor.foreground": "#171832",
        "notebook.focusedCellBorder": "#DBDAE2",
        "input.foreground": "#171832",
        "dropdown.background": "#C6C5CE",
        "button.foreground": "#C6C5CE",
        "editorGroupHeader.tabsBackground": "#DBDAE2",
        "editorGutter.modifiedBackground": "#427375",
        "icon.foreground": "#171832",
        "editor.background": "#F1F0F5",
        "diffEditor.diagonalFill": "#C6C5CE44",
        "activityBar.activeFocusBorder": "#1B5759",
        "activityBar.activeBorder": "#427375",
        "dropdown.border": "#BBBAC4",
        "statusBar.background": "#DBDAE2",
        "dropdown.listBackground": "#C6C5CE",
        "gitDecoration.untrackedResourceForeground": "#853261",
        "activityBar.foreground": "#171832",
        "gitDecoration.addedResourceForeground": "#25591B",
        "editor.selectionBackground": "#C6C5CE66",
        "editorGutter.addedBackground": "#4A7442",
        "editor.lineHighlightBorder": "#F1F0F5",
