        "input.placeholderForeground": "#C6C5CE",
        "input.background": "#F1F0F5",
        "sideBar.border": "#DBDAE2",
        "sideBarSectionHeader.foreground": "#171832",
        "notebook.cellHoverBackground": "#DBDAE255",
        "sideBarStickyScroll.shadow": "#D0CFD866",
        "gitDecoration.modifiedResourceForeground": "#1B5759",
        "editorLineNumber.foreground": "#D0CFD8",
        "window.activeBorder": "#DBDAE2",
        "foreground": "#171832",
        "notebook.focusedEditorBorder": "#543DA1",
        "editorCursor.foreground": "#705DB0",
