{
    "name": "One O Eight Forest",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#5C6A5C"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#919B91"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#405432"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#65755A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#225846"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#3E5B20"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#5C6A5C",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#5C6A5C"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#7D724A"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#768276"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#647B4C"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#7E461C"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#976A48"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#5E501E",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#5E501E"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#7E461C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#7E461C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#5E501E"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#5E501E",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#7E461C",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#3E5B20",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#65755A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#7D724A",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#7E461C",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#5C6A5C",
                "fontStyle": "underline"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#919B91",
        "tab.unfocusedActiveBackground": "#FBFBFB",
        "tab.inactiveBackground": "#D3D3D3",
        "notebookStatusErrorIcon.foreground": "#4D786A",
        "tab.border": "#E7E7E7",
        "editor.foreground": "#273A27",
        "notebook.focusedCellBorder": "#E7E7E7",
        "editorGroupHeader.tabsBackground": "#E7E7E7",
        "editorGutter.modifiedBackground": "#7D724A",
        "icon.foreground": "#5C6A5C",
        "editor.background": "#FBFBFB",
        "diffEditor.diagonalFill": "#ACB3AC44",
        "statusBar.background": "#E7E7E7",
        "gitDecoration.untrackedResourceForeground": "#7E461C",
        "activityBar.foreground": "#5C6A5C",
        "gitDecoration.addedResourceForeground": "#3E5B20",
        "editor.selectionBackground": "#919B9166",
        "editorGutter.addedBackground": "#647B4C",
        "editor.lineHighlightBorder": "#FBFBFB",
        "notebookStatusSuccessIcon.foreground": "#647B4C",
        "editorGutter.deletedBackground": "#4D786A",
        "titleBar.activeBackground": "#E7E7E7",
        "sideBar.border": "#E7E7E7",
        "notebook.cellHoverBackground": "#E7E7E755",
        "gitDecoration.modifiedResourceForeground": "#5E501E",
        "editorLineNumber.foreground": "#ACB3AC",
        "foreground": "#273A27",
        "notebook.focusedEditorBorder": "#7E461C",
        "editorCursor.foreground": "#405432",
        "sideBar.background": "#E7E7E7",
        "notebookStatusRunningIcon.foreground": "#7D724A",
        "list.highlightForeground": "#273A27",
        "tab.activeBorderTop": "#976A48",
        "notebook.focusedCellBackground": "#E7E7E7",
        "sideBar.dropBackground": "#976A48",
        "statusBar.foreground": "#273A27",
        "editorLineNumber.activeForeground": "#768276",
        "diffEditor.removedTextBackground": "#65755A22",
        "titleBar.inactiveBackground": "#E7E7E7",
        "diffEditor.insertedTextBackground": "#7D724A22",
        "activityBar.background": "#E7E7E7",
        "editorGroup.border": "#E7E7E7",
        "gitDecoration.deletedResourceForeground": "#225846"
    },
    "type": "light"
}
