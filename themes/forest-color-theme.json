{
    "name": "Forest",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#6E7B6E"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#305A33"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#497566",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#225846"
            }
        },
        {
            "name": "Constant (numeric)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#3E5B20"
            }
        },
        {
            "name": "Constant (escape char)",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#4B594A",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#4B594A"
            }
        },
        {
            "name": "Operator (boolean)",
            "scope": [
                "keyword.operator.boolean"
            ],
            "settings": {
                "foreground": "#7A6F46"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.bracket"
            ],
            "settings": {
                "foreground": "#6E7B6E"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#607847"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#7E461C"
            }
        },
        {
            "name": "Interpolation",
            "scope": [
                "variable.interpolation"
            ],
            "settings": {
                "foreground": "#946644"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#5E501E",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function in code",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#5E501E"
            }
        },
        {
            "name": "TOML Section",
            "scope": [
                "entity.other.attribute-name.table.array.toml",
                "entity.other.attribute-name.table.toml"
            ],
            "settings": {
                "foreground": "#7E461C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Name tag entity",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#7E461C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Quoted YAML string",
            "scope": [
                "string.quoted.single.yaml",
                "string.quoted.double.yaml"
            ],
            "settings": {
                "foreground": "#5E501E"
            }
        },
        {
            "name": "Markdown title",
            "scope": [
                "markup.heading.markdown"
            ],
            "settings": {
                "foreground": "#5E501E",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown fenced block language",
            "scope": [
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#7E461C",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#3E5B20",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link reference",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#547757",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown actual link",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#7A6F46",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown raw code",
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#7E461C",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown citation",
            "scope": [
                "string.other.link.description.markdown.citation"
            ],
            "settings": {
                "foreground": "#4B594A",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#919B91"
            }
        }
    ],
    "colors": {
        "gitDecoration.ignoredResourceForeground": "#CED2CD",
        "tab.unfocusedActiveBackground": "#FCFBFB",
        "tab.inactiveBackground": "#E0E2E0",
        "notebookStatusErrorIcon.foreground": "#497566",
        "tab.border": "#EAEAE9",
        "badge.foreground": "#273A27",
        "badge.background": "#7A6F46",
        "editor.foreground": "#273A27",
        "notebook.focusedCellBorder": "#EAEAE9",
        "editorGroupHeader.tabsBackground": "#EAEAE9",
        "editorGutter.modifiedBackground": "#7A6F46",
        "icon.foreground": "#273A27",
        "editor.background": "#FCFBFB",
        "diffEditor.diagonalFill": "#D8DAD644",
        "statusBar.background": "#EAEAE9",
        "gitDecoration.untrackedResourceForeground": "#225846",
        "activityBar.foreground": "#273A27",
        "gitDecoration.addedResourceForeground": "#3E5B20",
        "editor.selectionBackground": "#D8DAD666",
        "editorGutter.addedBackground": "#607847",
        "editor.lineHighlightBorder": "#FCFBFB",
        "notebookStatusSuccessIcon.foreground": "#607847",
        "editorGutter.deletedBackground": "#497566",
        "titleBar.activeBackground": "#EAEAE9",
        "sideBar.border": "#EAEAE9",
        "notebook.cellHoverBackground": "#EAEAE955",
        "gitDecoration.modifiedResourceForeground": "#5E501E",
        "editorLineNumber.foreground": "#E0E2E0",
        "foreground": "#273A27",
        "notebook.focusedEditorBorder": "#7E461C",
        "editorCursor.foreground": "#C5C9C4",
        "activityBar.inactiveForeground": "#C5C9C4",
        "sideBar.background": "#EAEAE9",
        "notebookStatusRunningIcon.foreground": "#7A6F46",
        "list.highlightForeground": "#273A27",
        "tab.activeBorderTop": "#7A6F46",
        "notebook.focusedCellBackground": "#EAEAE9",
        "sideBar.dropBackground": "#7A6F46",
        "statusBar.foreground": "#273A27",
        "editorLineNumber.activeForeground": "#C5C9C4",
        "diffEditor.removedTextBackground": "#54775722",
        "titleBar.inactiveBackground": "#EAEAE9",
        "diffEditor.insertedTextBackground": "#7A6F4622",
        "activityBar.background": "#EAEAE9",
        "editorGroup.border": "#EAEAE9",
        "gitDecoration.deletedResourceForeground": "#225846"
    },
    "type": "light"
}
